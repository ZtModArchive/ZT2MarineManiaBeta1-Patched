   <binder>
      <BFBinder>
         <instance>
            <ZTTankGateSegment ladderType="tankladder_show" platformType="TankPlatform_show" support="tankpost_show" middleSupport="tanksupport_show" maxUnsupportedSpan="3" maxUnsupportedDiagSpan="2" skipPostsOnStraight="false"/>
         </instance>
      </BFBinder>
      <BFNamedBinder binderName="start_post">
         <instance>
            <BFPhysObj>
