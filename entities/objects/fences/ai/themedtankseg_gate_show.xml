      </BFNamedBinder>
      <BFNamedBinder required="0" binderName="fence45">
         <instance>
            <BFPhysObj>
               <BFSceneGraphComponent isBFR="true" modelfile="entities\objects\fences\ThemedTank_show\ThemedTank_show_curb_diag" switch="0 30"/>
