            </BFNamedBinder>
            <BFNamedBinder binderName="water" required="1">
               <instance>
                  <BFLocoAnimate version="1" stationaryAnim="TreadWater_Idle">
                     <stop name="TreadWater"/>
                     <slow name="Swim"/>
                     <medium name="Swim"/>
                     <fast name="Swim"/>
                  </BFLocoAnimate>
               </instance>
            </BFNamedBinder>
         </binder>
      </BFBinder>
      <BFNamedBinder binderName="Broom" required="0">
