<BFAITaskTemplateList>
   <BFAITaskTemplate Name="Training" Priority="85" reserveTag="Training" UniqueID="TrickPlatform:Training">
     <BFAICreateData>
       <Subjects>
          <WalrusPacific_Adult/>
          <Qualifiers inWater_OR_onLand="true"/>
       </Subjects>
       <Targets>
         <fromToken/>
         <Qualifiers_AND inWater="true"/>
       </Targets>
       <Objects>
         <t_FPTSummonWalrusPacificBellow/>
         <t_FPTSummonWalrusPacificBellow_Male/>
       </Objects>
     </BFAICreateData>
     <BFAIEvalData fixedScore="95"/>
     <BFBehExecTask>
        <BFBehDockSpline locoSpeed="veryfast"/>
        <BFBehDockSpline locoSpeed="veryfast" targetNode="Node_Training" targetAnim="TreadWater_Idle" redock="true"/>
     </BFBehExecTask>
     <BFAICompletionData/>
     <BFAIFailureData/>
   </BFAITaskTemplate>
   <BFAITaskTemplate Name="Bellow_Failure" reserveTag="Bellow_Failure" UniqueID="TrickPlatform:Bellow_Failure">
     <BFAICreateData>
       <Subjects>
          <WalrusPacific_Adult/>
         <Qualifiers isSwimming="true"/>
       </Subjects>
       <Targets>
         <fromToken/>
         <Qualifiers inWater="true"/>
       </Targets>
       <Objects>
         <t_BellowFailure/>
       </Objects>
     </BFAICreateData>
     <BFAIEvalData fixedScore="2000"/>
     <BFBehExecTask>
       <BFBehEnter/>
       <BFBehPlaySet behSet="useContainer"/>
     </BFBehExecTask>
     <BFAICompletionData>
      <BFAIAttributeFloatMap exercise="-5" stimulation="-5"/>
       <BFBehExecTask>
          <ZTBehFeedback>
             <ZTFeedbackData>
                <ZTEmoticonInfo emoticonName="EmoteAnimalShowBad"/>
             </ZTFeedbackData>
          </ZTBehFeedback>
          <BFBehSendToken>
              <BFAITokenList>
                 <BFAIToken Name="t_FailureReaction" GiveTo="MC" Payload="subject" Radius="50" Timeout="10" Reconsider="true"/>
              </BFAITokenList>
           </BFBehSendToken>
        </BFBehExecTask>
     </BFAICompletionData>
     <BFAIFailureData>
       <BFBehExecTask>
         <BFBehPlaySet behSet="FailureSet"/>
       </BFBehExecTask>
     </BFAIFailureData>
   </BFAITaskTemplate>
