<ZT2Strings>
      <LOC_STRING _locID="zoopedia_showprops:entry"  >Show Props</LOC_STRING>
      <LOC_STRING _locID="zoopedia_showprops:text"  >
		<cell pady="0" width="1000"/>
		<p/>
		<cell width="260"/>
		<cell width="640" height="320" bgimg="ui/zoopedia/topCellBg.dds">
		 <cell width="600" padx="20" pady="20">
		  <color r="255" g="255" b="255">
			<p/>Show props are objects used by animals when performing a trick in a show. Show props appear automatically when the trick is ready to begin.
			<p/>To view information about a specific show prop, click the name of the show prop in the list on the left.	  
		  </color>
		 </cell>
		</cell>
	  </LOC_STRING>
